  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Aktuelle Datei",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },{
      "name": "Python-Debugger: Aktuelle Datei mit Argumenten",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "${command:pickArgs}"
    },
  ]
}
